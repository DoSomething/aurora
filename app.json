{
  "name": "aurora",
  "description": ":crescent_moon:",
  "addons": [
    "jawsdb-maria",
    "heroku-redis",
    "papertrail"
  ],
  "buildpacks": [
    {
      "url": "heroku/php"
    },
    {
      "url": "heroku/nodejs"
    }
  ],
  "env": {
    "APP_DEBUG": "false",
    "NPM_CONFIG_PRODUCTION": "false",
    "APP_KEY": {
      "required": true
    },
    "APP_LOG": "errorlog",
    "NORTHSTAR_AUTH_ID": {
      "required": true
    },
    "NORTHSTAR_AUTH_SECRET": {
      "required": true
    },
    "NORTHSTAR_URL": {
      "required": true
    }
  }
}
